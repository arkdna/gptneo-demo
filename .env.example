# VCD Configuration
VCD_USER=administrator
VCD_PASSWORD=your-vcd-password
VCD_ORG=your-org-name
VCD_VDC=your-vdc-name
VCD_URL=https://your-vcd-instance.example.com/api

# Network Configuration
NETWORK_SEGMENT=default-network
DEFAULT_GATEWAY=192.168.1.1
DNS_SERVERS=["8.8.8.8", "8.8.4.4"]
VM_IP="192.168.1.100"

# VM Configuration
VM_NAME=gpt-neo-inference-demo
TEMPLATE_NAME=ubuntu-template

# Hardware Configuration
VM_MEMORY=32768
VM_CPUS=16
VM_CPU_CORES=1
VM_CPU_HOT_ADD_ENABLED=true
VM_MEMORY_HOT_ADD_ENABLED=true
VM_CATALOG_NAME="Local Catalog"

# SSH Configuration
SSH_PUBLIC_KEY="ssh-rsa AAAA... your-key-here"